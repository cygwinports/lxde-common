  /usr/share/doc/lxde-common-0.3.2.1/README
  /usr/share/icons/nuoveXT2/*/*/*
  /usr/share/lxde/config
  /usr/share/lxde/images/logout-banner.png
  /usr/share/lxde/images/lxde-icon.png
  /usr/share/lxde/openbox/menu.xml
